REM Display photo in SD card
REM BMP format(320x240/8bit-color)
WIDTH 40,2
USEGRAPHIC
GCLS
FOPEN "COSMOS.BMP","r"
FSEEK 54
FOR C=0 TO 255
 B=FGETC():G=FGETC()
 R=FGETC():D=FGETC()
 GPALETTE C,R,G,B
NEXT
FOR Y=239 TO 0 STEP -1
 FOR X=0 TO 319
  D=FGETC()
  PSET X,Y,D
 NEXT
NEXT
FCLOSE
WHILE KEYS()=0:WEND
END
